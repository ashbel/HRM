</ul>

<div class="tab-content">
  <div id="home" class="tab-pane fade in active">
   <fieldset>
<div class="row">
  <div class="col-xs-12 col-md-1">
   <div class="btn-group btn-group-xs">
       <br />
      <a  data-toggle="modal" href="#uploadModal">
          @{if (@ViewBag.url != null) {
          <img class="img-responsive" src="data:image;base64,@System.Convert.ToBase64String(ViewBag.url)" width ="140" height="140" />
              }
              else {
          <img class="img-responsive" src="" width="140" height="140" />
                  }
              }
      </a>
   </div>    
  </div>
  <div class="col-xs-12 col-md-4">
    <h3>Basic Information</h3>
    <hr />
        <div class="row">
            <div class="col-xs-6 col-md-6"><b> Employee Id:</b></div>
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_id)</div>
        </div>
        <div class="row">
           <div class="col-xs-6 col-md-6"><b>Employee Name:</b></div> 
           <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_name) @Html.DisplayFor(model => model.emp_lname)</div>
         </div>
        <div class="row">
            <div class="col-xs-6 col-md-6"><b>ID Number:</b></div>
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_id_num)</div>
        </div>
        <div class="row">
          <div class="col-xs-6 col-md-6"><b>Nationality:</b></div>
           <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_nation)</div>
         </div>
        <div class="row">
          <div class="col-xs-6 col-md-6"><b>Date of Birth:</b></div>
           <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_bDate)</div>
         </div>
    <hr />
    <h3>Employment Information</h3>
        <div class="row">
            <div class="col-xs-6 col-md-6"><b> EC Number:</b></div>
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_ec_num)</div>
        </div>
        <div class="row">
           <div class="col-xs-6 col-md-6"><b>Branch:</b></div> 
           <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_branch)</div>
         </div>
        <div class="row">
            <div class="col-xs-6 col-md-6"><b>Department:</b></div>
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_dpt)</div>
        </div>
        <div class="row">
          <div class="col-xs-6 col-md-6"><b>Employment Type</b></div>
           <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_status)</div>
         </div>
        <div class="row">
            <div class="col-xs-6 col-md-6"><b>Position:</b></div>
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_pos_id)</div>
        </div>
        <div class="row">
          <div class="col-xs-6 col-md-6"><b>Employee Start Date:</b></div>
          <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_jDate)</div>
        </div>
        <div class="row">
          <div class="col-xs-6 col-md-6"><b>Employee Status:</b></div>
          <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_state)</div>
        </div>
    <hr />
    <h3>Contact Information</h3>
        <div class="row">
            <div class="col-xs-6 col-md-6"><b> Email Address:</b></div>
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_email)</div>
        </div>
        <div class="row">
            <div class="col-xs-6 col-md-6"><b>Street Address:</b></div> 
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_strt)</div>
            </div>
        <div class="row">
            <div class="col-xs-6 col-md-6"><b>Surburb:</b></div>
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_nghbhd)</div>
        </div>
        <div class="row">
            <div class="col-xs-6 col-md-6"><b>City:</b></div>
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_city)</div>
            </div>
        <div class="row">
            <div class="col-xs-6 col-md-6"><b>Province:</b></div>
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_province)</div>
        </div>
        <div class="row">
            <div class="col-xs-6 col-md-6"><b>Phone Number:</b></div>
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_phone)</div>
        </div>
        <div class="row">
            <div class="col-xs-6 col-md-6"><b>Mobile Phone:</b></div>
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_mobile)</div>
        </div>
    <hr />
    <h3>Other Information</h3>
        <div class="row">
            <div class="col-xs-6 col-md-6"><b>Next Of Kin:</b></div>
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_kin)</div>
            </div>
        <div class="row">
            <div class="col-xs-6 col-md-6"><b>Next Of Kin Address:</b></div>
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_kin_add)</div>
        </div>
        <div class="row">
            <div class="col-xs-6 col-md-6"><b>Nex of Kin Phone Number:</b></div>
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_kin_ph)</div>
        </div>
        <div class="row">
            <div class="col-xs-6 col-md-6"><b>Relationship:</b></div>
            <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_kin_rel)</div>
        </div>
    <hr />
    <h3>Additional Information</h3>
    <div class="row">
        <div class="col-xs-6 col-md-6"><b>Create Date:</b></div>
        <div class="col-xs-6 col-md-6">@Html.DisplayFor(model => model.emp_cDate)</div>
    </div>
    <hr />
  </div>
  <div class="col-xs-12 col-md-7">
  <h3>User Activity</h3>
    <hr />
 <div class="row">
  <div class="col-sm-12 col-md-3">
    <div class="thumbnail thumbnail-info">
        <h5 class="text-center">Duration of Employment</h5>
        <h2 class="text-center">@ViewBag.Duration</h2>
    </div>
  </div>
  <div class="col-sm-12 col-md-3">
    <div class="thumbnail">
        <h5 class="text-center">Loans Balance</h5>
        <h2 class="text-center">$1200.50</h2>
    </div>
    </div>
  <div class="col-sm-12 col-md-3">
    <div class="thumbnail">
        <h5 class="text-center">YTD Earnings</h5>
        <h2 class="text-center">$8546.50</h2>
    </div>
    </div>
  <div class="col-sm-12 col-md-3">
    <div class="thumbnail thumbnail-info">
        <h5 class="text-center">Leave Days Balance</h5>
        <h2 class="text-center">12 Days</h2>
    </div>
  </div>
 </div>
</div>
</div>

</fieldset>
  </div>
  <div id="menu1" class="tab-pane fade">
      <h3>All Contracts</h3>
      <div class="btn-group btn-group-xs">
          <button class="btn btn-default btn-xs" data-toggle="modal" href="#addContract">Add Contracts</button>
      </div>
      <br /><p></p>
      <table class="table table-striped table-hover table-bordered table-condensed" id="table2" >
        <thead>
          <tr>
        <th>
           <label class="text-right">Title</label> 
        </th>
        <th>
           Start Date
        </th>
        <th>
           End Date
        </th>
        <th>
           State
        </th>
              <th>
                  Branch 
              </th>
              <th>
                  Department
              </th>
              <th>
                  Position
              </th>

     </tr>
        </thead>
        @{ if (ViewBag.Contracts != null)
            {

                foreach (var item in ViewBag.Contracts)
                {
         <tr>
        <td>
            <span data-placement="left" data-toggle="tooltip" title="Edit">
                <a href="#editContract" class="editContract" data-toggle="modal" data-id='["@item.cntrct_title","@item.cntrct_sdate","@item.cntrct_edate","@item.cntrct_state","@item.cntrct_desc","@item.dpt_id","@item.br_id","@item.pos_id"]'> <span class="glyphicon glyphicon-pencil"></span></a>
            </span>
            <span data-placement="left" data-toggle="tooltip" title="View">
                <a href="#viewContract" class="viewContract" data-toggle="modal" data-id='["@item.cntrct_title","@item.cntrct_sdate","@item.cntrct_edate","@item.tblContractState.cntrct_state","@item.cntrct_desc","@item.tblDepartment.dpt_name","@item.tblBranch.br_name","@item.tblPosition.pos_title"]'> <span class="glyphicon glyphicon-check"> </span></a>
            </span>
            <span data-placement="left" data-toggle="tooltip" title="Delete">
                <a href="#deleteContract" class="deleteContract" data-toggle="modal" data-id='["@item.cntrct_title","@item.cntrct_id","@item.tblPosition.pos_title"]'> <span class="glyphicon glyphicon-trash"></span> </a>
            </span>
                @item.cntrct_title
</td>
        <td>
             @String.Format("{0:dd-MMM-yyyy}", item.cntrct_sdate)
        </td>
        <td>
            @{ try { String.Format("{0:dd-MMM-yyyy}", item.cntrct_edate); } catch { } }
        </td>
        <td>
             @item.tblContractState.cntrct_state
        </td>
             <td>
                 @item.tblBranch.br_name
             </td>
             <td>
                 @item.tblDepartment.dpt_name
             </td>
             <td>
                 @item.tblPosition.pos_title 
             </td>
    </tr>
                       
          }
               
           }
        }
        </table>
  </div>
  <div id="menu2" class="tab-pane fade">
      <h3>Education</h3>
      <div class="btn-group btn-group-xs">
          <button class="btn btn-default btn-xs" data-toggle="modal" href="#addEducation" >Add Qualifications</button>
      </div>
      <br /><p></p>

      <table class="table table-striped table-hover table-bordered table-condensed" id="table1" >
        <thead>
          <tr>
        <th>
           Institution Name
        </th>
        <th>
            Level 
        </th>
        <th>
           Qualifications
        </th>
        <th>
           Score
        </th>
              <th>
                  Start Date
              </th>
              <th>
                 End Date
              </th>
     </tr>
        </thead>
        @{ if(ViewBag.education != null){

                foreach(var item in ViewBag.education)
                {
         <tr>
        <td>
            <a href="#editContract" data-toggle="modal" data-id=""> <span class="glyphicon glyphicon-pencil"></span></a>
            <a href="#viewContract" class="viewContract" data-toggle="modal" data-id=""> <span class="glyphicon glyphicon-check"> </span></a>
            <a href="#deleteContract" class="deleteContract" data-toggle="modal" data-id=""> <span class="glyphicon glyphicon-trash"></span> </a>
            @item.edu_name
        </td>
        <td>
            @item.tblEduLevel.edu_lv_name
        </td>
        <td>
             @item.edu_qlfcn
        </td>
        <td>
            @item.edu_score
        </td>
@try
{
    <td>
        @String.Format("{0:dd-MMM-yyyy}", item.edu_sdate)
    </td>
}
catch
{
     }
@try
{
    <td>
        @String.Format("{0:dd-MMM-yyyy}", item.edu_edate)
    </td>
}
catch
{
            }

    </tr>
                       
          }
               
           }
        }
        </table>
  </div>
  <div id="menu3" class="tab-pane fade">
      <h3>Work Experience</h3>
      <div class="btn-group btn-group-xs">
          <button class="btn btn-default btn-xs" data-toggle="modal" href="#addWork">Add Work Experience</button>
      </div>
