                                          <div class="col-md-3 col-sm-12">
                                              <div class="btn-group btn-group-sm">
                                                  <a class="btn btn-default identifyingClass" data-id='["@item.course_nm","@item.course_id"]' data-toggle="modal" href="#enrolCourse" ><div style="color:#2facc2">Enroll</div></a>
                                              </div>
                                              <div class="btn-group btn-group-sm">
                                                  <a class="btn btn-default viewCourse" data-id='["@item.course_nm","@item.tblCourseType.type_name","@item.course_dur","@item.course_descr"]' data-toggle="modal" href="#viewCourse"><div style="color:#2facc2">View Details</div></a>
                                              </div>
                                          </div>
                                      </div>
                                  </td>


                              </tr>

                            }

                        }
                          }
                      </table>
                  </div>
              </div>
          </div>
