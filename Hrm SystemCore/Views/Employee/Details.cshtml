@model Hrm_SystemCore.Models.EmployeeModelView

@{
    ViewBag.Title = " HRM - Employee Details";
}
<h3> @Html.DisplayFor(model => model.emp_name) @Html.DisplayFor(model => model.emp_lname) </h3> 
<ul class="nav nav-tabs">
  <li class="active"><a data-toggle="tab" href="#home"><b>Employee Details</b></a></li>
  <li><a data-toggle="tab" href="#menu1"><b>Contract Information</b></a></li>
  <li><a data-toggle="tab" href="#menu2"><b>Education</b></a></li>
  <li><a data-toggle="tab" href="#menu3"><b>Work Experience</b></a></li>
  <li><a data-toggle="tab" href="#menu4"><b>Trainings</b></a></li>
  <li><a data-toggle="tab" href="#menu5"><b>Achievements</b></a></li>
  <li><a data-toggle="tab" href="#menu6"><b>Performance Evaluation</b></a></li>
  <li><a data-toggle="tab" href="#menu9"><b>Documents</b></a></li>
