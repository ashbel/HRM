@model IEnumerable<Hrm_System.Models.tblEduLevel>

    @{
        ViewBag.Title = "HRM - Education Levels";
    }

    <h2>Education Levels</h2>

    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table table-striped table-hover table-bordered table-condensed " id="table1">
        <thead>
            <tr>
                <th>
                    Education Level
                </th>
                <th></th>
            </tr>
</thead>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.edu_lv_name)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.edu_lv_id }) |
                        @Html.ActionLink("Details", "Details", new { id = item.edu_lv_id }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.edu_lv_id })
                    </td>
                </tr>
            }

    </table>
